{
  "name": "tree-sitter-jsp",
  "version": "0.1.0",
  "description": "JSP grammar for tree-sitter",
  "keywords": [
    "parser",
    "lexer"
  ],
  "main": "index.js",
  "repository": "https://github.com/merico-dev/tree-sitter-jsp",
  "homepage": "https://github.com/merico-dev/tree-sitter-jsp#readme",
  "author": {
    "name": "QinTianHuan",
    "email": "tianhuan.qin@merico.dev",
    "url": "https://meri.co"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn tree-sitter test",
    "prepack": "bash scripts/update-html-scanner.sh && yarn tree-sitter generate",
    "release": "standard-version --commit-all",
    "tree-sitter": "./tree-sitter/target/release/tree-sitter"
  },
  "standard-version": {
    "preset": "angular",
    "scripts": {
      "postbump": "rm -r docs && node scripts/generate-playground.js && git add docs"
    }
  },
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "standard-version": "7.0.0",
    "tree-sitter-html": "0.15.0"
  },
  "files": [
    "/src/",
    "/binding.gyp",
    "/grammar.js",
    "/index.js",
    "/ThirdPartyNoticeText.txt"
  ]
}